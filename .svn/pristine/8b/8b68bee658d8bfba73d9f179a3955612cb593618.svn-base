<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >
 <LinearLayout
     android:layout_width="200dp"
     android:layout_height="wrap_content"
     android:id="@+id/ll_popubody"
     android:background="@drawable/popu_web"
     android:orientation="vertical"
     >
     <TextView
         android:layout_gravity="center"
         android:layout_marginTop="10dp"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         style="@style/style_app_popucom"
         android:id="@+id/tv_popufirst"
         android:text="文件共享"
         ></TextView>
     <View
         android:layout_width="match_parent"
         android:layout_height="1dp"
           android:layout_marginTop="10dp"
         android:background="@drawable/list_separator"
         ></View>
     <TextView 
          android:layout_gravity="center"
         android:layout_marginTop="10dp"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         style="@style/style_app_popucom"
         android:id="@+id/tv_popusecond"
         android:text="wifi列表"
         />
     <View
         android:layout_width="match_parent"
         android:layout_height="1dp"
           android:layout_marginTop="10dp"
         android:background="@drawable/list_separator"
         ></View>
     <TextView 
          android:layout_gravity="center"
         android:layout_marginTop="10dp"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         style="@style/style_app_popucom"
         android:id="@+id/tv_popthir"
         android:layout_marginBottom="10dp"
         android:text="网络状态"
         />
 </LinearLayout>
</RelativeLayout>
